# Sample gitconfig
#

[hub]
    protocol = https
[user]
    name = riijagraphics
    email = riijagraphics@gmail.com
[credential]
    username = riijagraphics
    helper = store
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[core]
    editor = vim
    ignorecase = false
    excludesfile = ~/.gitignore
    pager = diff-so-fancy | less --tabs=1,5 -RFX
[apply]
    whitespace = nowarn
[mergetool]
    keepBackup = false
[difftool]
    prompt = false
[help]
    autocorrect = 1
[push]
    default = current
[pull]
    rebase = true
